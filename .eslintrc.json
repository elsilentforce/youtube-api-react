{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "react/jsx-filename-extension": [
            1,
            { "extensions": [".js", ".jsx"] }
        ]
    }
}
